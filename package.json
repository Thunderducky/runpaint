{
  "name": "runpaint",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Eric Scott",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:development": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
    "start:production": "node server.js",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest ./server",
    "lint": "eslint ./"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "per-env": "^1.0.2",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint": "^5.5.0",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-jest": "^21.22.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4"
  }
}
